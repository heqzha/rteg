#!/usr/bin/env python
"""
@file		Poly.py
@author		Hequn Zhang
@date		30-04-2013
@version	0.1

Definition of polygons
"""

class Poly:
	def __init__(self, iId, iColor, iShape, iFill = 1, iLayer = 0, iType = "building", iImageFile = None):
		self.mId = iId
		self.mColor = iColor
		self.mShape = iShape
		self.mFill = iFill
		self.mLayer = iLayer
		self.mType = iType
		self.mImageFile = iImageFile

	def getId(self):
		return self.mId
	
	def getColor(self):
		return self.mColor
	
	def getShape(self):
		return self.mShape

	def getFill(self):
		return self.mFill

	def getLayer(self):
		return self.mLayer

	def getType(self):
		return self.mType

	def getImageFile(self):
		return self.mImageFile

